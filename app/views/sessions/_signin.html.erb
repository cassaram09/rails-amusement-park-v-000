<%= form_for user, controller: 'sessions', action: 'create', method: 'post' do |f|%>
  <p>
    <%= f.label "Name" %>
    <%= f.collection_select(:name, User.all, :name, :name) %>
  </p>
  <p>
    <%= f.label "Password" %>
    <%= f.password_field :password  %>
  </p>
  <%= f.submit "Sign in" %>
<% end %>